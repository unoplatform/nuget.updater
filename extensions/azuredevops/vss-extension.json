{
	"manifestVersion": 1,
	"id": "unoplatform-build-tools",
	"version": "0.0.0",
	"name": "Uno Platform Build Tools",
	"description": "The Canary Updater tool by Uno Platform",
	"publisher": "unoplatform",
	"public": true,
	"tags": [
		"Extension",
		"Marketplace",
		"Package",
		"Install",
		"Build",
		"Azure Pipelines",
		"Azure DevOps Extensions"
	],
	"targets": [
		{
			"id": "Microsoft.VisualStudio.Services"
		}
	],
	"categories": [
		"Azure Pipelines"
	],
	"icons": {
		"default": "logo.png"
	},
	"content": {
		"details": {
			"path": "overview.md"
		}
	},
	"repository": {
		"type": "git",
		"uri": "https://github.com/unoplatform/nuget.updater"
	},
	"contributions": [
		{
			"id": "unoplatform.canaryUpdater",
			"type": "ms.vss-distributed-task.task",
			"targets": [
				"ms.vss-distributed-task.tasks"
			],
			"properties": {
				"name": "canaryUpdater"
			}
		}
	],
	"scopes": [],
	"files": [
		{
			"path": "canaryUpdater"
		},
		{
			"path": "node_modules"
		}
	]
}
